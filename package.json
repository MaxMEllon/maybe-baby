{
  "name": "maybe-baby",
  "version": "1.0.0",
  "description": "Maybe monad for JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "npm run clear && npm run compile",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec",
    "compile": "babel src -d lib",
    "clear": "rm -rf ./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/maybe-baby.git"
  },
  "keywords": [
    "maybe",
    "monad",
    "javascript"
  ],
  "author": "Mike Chabot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikechabot/maybe-baby/issues"
  },
  "homepage": "https://github.com/mikechabot/maybe-baby#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3"
  },
  "tonicExampleFilename": "example.js"
}
